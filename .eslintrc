{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "no-unused-vars-rest"
  ],
  "env": {
    "browser": true,
    "jest": true,
    "jasmine": true
  },
  "globals": {
    "__DEV__": true,
    "__PROD__": true,
    "__DEBUG__": true,
    "__COVERAGE__": true,
    "__BASENAME__": true
  },
  "settings": {

  },
  "rules": {
    "semi": [1, "always"],
    "comma-dangle": [2, "always-multiline"],
    "no-restricted-syntax": 0,
    "max-len": 0,
    "no-shadow": 0,
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "global-require": 0,
    "no-unused-expressions": 0,
    "no-confusing-arrow": 0,
    "no-unused-vars": 0,
    "no-constant-condition": 0,
    "no-unused-vars-rest/no-unused-vars": [2, {"ignoreDestructuredVarsWithRest": true}]
  }
}
